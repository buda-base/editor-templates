# imports: http://purl.bdrc.io/ontology/core/
# prefix: bds

@prefix ItemShapes: <http://purl.bdrc.io/ontology/shapes/core/ItemShapes/>
@prefix adm: <http://purl.bdrc.io/ontology/admin/> .
@prefix bdd: <http://purl.bdrc.io/data/> .
@prefix bdo: <http://purl.bdrc.io/ontology/core/> .
@prefix bdr: <http://purl.bdrc.io/resource/> .
@prefix bds: <http://purl.bdrc.io/ontology/shapes/core/> .
@prefix bdsa: <http://purl.bdrc.io/ontology/shapes/adm/> .
@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix iiif2: <http://iiif.io/api/presentation/2#> .
@prefix iiif3: <http://iiif.io/api/presentation/3#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swa: <http://topbraid.org/swa#> .
@prefix tbr: <http://purl.bdrc.io/ontology/toberemoved/> .
@prefix tosh: <http://topbraid.org/tosh#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ItemShapes:
  a owl:Ontology ;
  owl:imports <http://datashapes.org/dash> ;
  owl:imports bdo: ;
  owl:versionInfo "Initial SHACL constraints for Item and its subclasses" ;
.
bds:ItemPhysicalAssetShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Item Shape"@en ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bf:heldBy ;
      sh:class bdo:Agent ;
      sh:minCount 0 ;
      sh:name "held by"@en ;
  ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bf:identifiedBy ;
      sh:inversePath bf:identifies ;
      sh:class bdo:ItemIdentifier ;
      sh:minCount 0 ;
      sh:name "identified by"@en ;
  ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:heldAt ;
      sh:class bdo:ItemIdentifier ;
      sh:minCount 0 ;
      sh:name "is held at"@en ;
  ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:notAfter ;
      sh:nodeKind xsd:gYear ;
      sh:maxCount 1 ;
      sh:minCount 0 ;
      sh:name "acquired not after"@en ;
  ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:notBefore ;
      sh:nodeKind xsd:gYear ;
      sh:maxCount 1 ;
      sh:minCount 0 ;
      sh:name "acquired not before"@en ;
  ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:onDate ;
      sh:nodeKind xsd:date ;
      sh:maxCount 1 ;
      sh:minCount 0 ;
      sh:name "acquired on"@en ;
  ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:onOrAbout ;
      sh:nodeKind xsd:string ;
      sh:maxCount 1 ;
      sh:minCount 0 ;
      sh:name "acquired on or about"@en ;
  ] ;
  sh:targetClass bdo:ItemPhysicalAsset ;
.
