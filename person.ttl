bds:FemaleShape
  a sh:NodeShape ;
  rdfs:label "FemaleShape"@en ;
  sh:property [
      sh:path bdo:gender ;
      sh:hasValue bdr:GenderFemale ;
    ] ;
  sh:targetClass bdo:Person ;
.
bds:GenderShape
  a sh:NodeShape ;
  rdfs:label "GenderShape"@en ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:gender ;
      sh:class bdo:Gender ;
      sh:description "this Person may have at most one Gender."@en ;
      sh:maxCount 1 ;
      sh:name "person has gender"@en ;
    ] ;
  sh:targetClass bdo:Person ;
.
bds:KinshipShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Kinship Shape"@en ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:hasBrother ;
      sh:class bdo:Person ;
      sh:description "this Person may have zero or more brothers."@en ;
      sh:inversePath bdo:hasSibling ;
      sh:minCount 0 ;
      sh:name "person has brother"@en ;
      sh:node bds:MaleShape ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path bdo:hasCousin ;
